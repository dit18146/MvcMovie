@model Movies;



<div>
    <table class="table table-hover" id="table">
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Description</th>
            <th> Category </th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var item in Model.Items)
        {
            <tr class="table-secondary text-secondary">
                <td>@item.Id</td>

                <td>@item.Title</td>

                <td>@item.Description</td>

                <td>@item.Name</td>
            </tr>
        }

    </table>
</div>